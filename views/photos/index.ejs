<%- include('../partials/header') %>
<table id="list">
  <thead>
    <tr>
      <th>Title</th>
      <th>Photograph</th>
      <th>Year Taken</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% photos.forEach(function(p) { %>
      <tr>
        <td><%= p.title %></td>
        <td><img src="<%= p.photoLink %>" width="500"></td>
        <td><%= p.yearTaken %></td>
        <td><a href="/photos/<%= p._id %>">Details</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>
